{
  "name": "desktop-app",
  "version": "0.0.0",
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron": "electron .",
    "electron-build": "ng build --prod && electron .",
    "rebuild": "electron-rebuild -f -w sqlite3",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . desktop-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=src/favicon.ico --prune=true --out=release-builds --version-string.CompanyName=RKATRE --version-string.FileDescription=POS-0.1 --version-string.ProductName='Offline POS'",
    "package-linux": "electron-packager . desktop-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.0",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/router": "^5.2.0",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0",
    "angular-bootstrap-md": "^5.1.2",
    "bootstrap": "^4.0.0-beta.3",
    "chart.js": "^2.5.0",
    "core-js": "^2.4.1",
    "electron": "^1.8.2",
    "electron-ipc-promise": "^0.1.7",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "knex": "^0.14.3",
    "ngx-electron": "^1.0.4",
    "rxjs": "^5.5.6",
    "sqlite3": "^3.1.13",
    "zone.js": "^0.8.5",
    "systemjs": "^0.20.19"
  },
  "devDependencies": {
    "@angular/cli": "1.6.6",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/language-service": "^5.2.0",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.1",
    "concurrently": "^3.1.0",
    "electron": "^1.8.2",
    "electron-builder": "^19.56.2",
    "electron-packager": "^8.7.2",
    "electron-rebuild": "^1.7.3",
    "electron-winstaller": "^2.6.4",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lite-server": "^2.3.0",
    "protractor": "~5.1.2",
    "source-map-explorer": "^1.5.0",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "~2.5.3"
  }
}
